{
  "name": "resume-generator",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/tailwind.css -o ./static/css/main.css",
    "watch-css": "tailwindcss -i ./static/css/tailwind.css -o ./static/css/main.css --watch",
    "start-server": "python3 resume-generator.py",
    "dev": "concurrently --kill-others \"npm run watch-css\" \"npm run start-server\""
  },
